<Window x:Class="OOP_5.Views.MVVMApp"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OOP_5"
        mc:Ignorable="d"
        Title="Practice 5" Height="450" Width="800" HorizontalAlignment="Right" VerticalAlignment="Bottom"
        Background="#DBECF4" ResizeMode="NoResize">
    <DockPanel>
        <Menu DockPanel.Dock="Top" Style="{StaticResource TopMenuStyle}" Width="802">
            <MenuItem Header="_Task1" Click="Task1_Click"></MenuItem>
            <MenuItem Header="_Task2" Click="Task2_Click"></MenuItem>
            <MenuItem Header="_Task3" Click="Task3_Click"></MenuItem>
        </Menu>
        <Frame Name="MainFrame" Source="Home.xaml" Style="{StaticResource MinimalFrameStyle}"/>
    </DockPanel>
</Window>
